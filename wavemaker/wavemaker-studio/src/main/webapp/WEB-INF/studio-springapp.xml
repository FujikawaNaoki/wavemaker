<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans 
           http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
           http://www.springframework.org/schema/context
           http://www.springframework.org/schema/context/spring-context-3.0.xsd">

	<context:component-scan base-package="com.wavemaker.tools" />

	<import resource="classpath:config.xml" />
	<import resource="classpath:managers.xml" />
	<import resource="classpath:security.xml" />
	<import resource="classpath:services.xml" />

	<import resource="studio-services.xml" />
	<import resource="studio-managers.xml" />
	<import resource="classpath:springapp.xml" />
	<import resource="classpath:**/*.spring.xml" />

	<import resource="studio-controllers.xml" />
	<import resource="studio-security-empty.xml" />
	<import resource="studio-partner-beans.xml" />
</beans>
